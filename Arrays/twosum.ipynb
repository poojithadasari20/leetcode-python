   "source": [
    "# Problem: Two Sum\n",
    "# Platform: LeetCode\n",
    "# Approach: Hash Map\n",
    "# Time Complexity: O(n)\n",
    "# Space Complexity: O(n)\n",
    "\n",
    "def twoSum(nums, target):\n",
    "    mp = {}\n",
    "    for i, num in enumerate(nums):\n",
    "        if target - num in mp:\n",
    "            return [mp[target - num], i]\n",
    "        mp[num] = i\n"
   ]
